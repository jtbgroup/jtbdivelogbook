<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="be.vds.jtbdive.core.model.PhysiologicalStatus" table="physiologicalstatuses">
		<id name="id" column="id" type="long">
			<generator class="increment" />
		</id>
		<property name="arterialMicrobubbleLevel" />
		<property name="saturationIndex" />
		<property name="maxPPO2" />
		<property name="skinCoolTemperature" />
		<property name="cnsBeforeDive" />
		<property name="interPulmonaryShunt" />
	</class>

</hibernate-mapping>